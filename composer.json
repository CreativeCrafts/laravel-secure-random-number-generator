{
    "name": "creativecrafts/laravel-secure-random-number-generator",
    "description": "A handy package to generate a secure unique random number for a model",
    "keywords": [
        "CreativeCrafts",
        "laravel",
        "laravel-secure-random-number-generator"
    ],
    "homepage": "https://github.com/creativecrafts/laravel-secure-random-number-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Godspower Oduose",
            "email": "infor@rockblings.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CreativeCrafts\\LaravelSecureRandomNumberGenerator\\": "src/",
            "CreativeCrafts\\LaravelSecureRandomNumberGenerator\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CreativeCrafts\\LaravelSecureRandomNumberGenerator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreativeCrafts\\LaravelSecureRandomNumberGenerator\\LaravelSecureRandomNumberGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelSecureRandomNumberGenerator": "CreativeCrafts\\LaravelSecureRandomNumberGenerator\\Facades\\LaravelSecureRandomNumberGenerator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}